{
  "name": "task-app",
  "description": "A comprehensive task management app",
  "version": "0.1.0",
  "keywords": [
    "nextjs",
    "nodejs"
  ],
  "repository": {
    "url": "https://github.com/copilot-platforms/tasks-app"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:grant-supabase-privileges": "node src/lib/supabase-privilege",
    "vercel-build": "prisma generate && next build",
    "prepare": "husky install",
    "eslint": "eslint './src/**/*.{ts,tsx}'",
    "lint:check": "eslint \"{src,test}/**/*.{ts,tsx}\"",
    "lint:fix-stylish": "cp ./src/lib/stylish.js ./node_modules/eslint/lib/cli-engine/formatters/",
    "lint:fix": "npm run lint:check -- --fix",
    "prettier:check": "prettier --check \"{src,test}/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write .",
    "lint-staged": "npx lint-staged",
    "postinstall": "prisma generate",
    "test": "jest",
    "loadtest": "tsx ./src/cmd/load-testing",
    "loadtest:delete-clients": "tsx ./src/cmd/load-testing/deleteClients.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.6",
    "@mui/x-date-pickers": "^7.1.1",
    "@prisma/client": "^5.12.0",
    "@reduxjs/toolkit": "^2.2.3",
    "@sentry/nextjs": "^8",
    "@supabase/supabase-js": "^2.43.4",
    "@types/date-fns": "^2.6.0",
    "@vercel/blob": "^0.23.2",
    "@vercel/postgres": "^0.8.0",
    "bottleneck": "^2.19.5",
    "copilot-node-sdk": "^3.2.0",
    "dayjs": "^1.11.10",
    "http-status": "^1.7.4",
    "immutability-helper": "^3.1.1",
    "next": "14.2.6",
    "nextjs-progressloader": "^1.2.0",
    "prisma": "^5.12.0",
    "react": "18.3.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18",
    "react-redux": "^9.1.0",
    "tapwrite": "^1.0.60",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@svgr/webpack": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-custom-scrollbars": "^4.0.13",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.16.5",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "yarn prettier:fix"
    ]
  }
}
